{
  "name": "fetch-politely",
  "version": "2.0.0-3",
  "description": "Ensures polite outgoing HTTP requests that respects robots.txt and aren't made too close to each other",
  "homepage": "http://github.com/voxpelli/node-fetch-politely",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/node-fetch-politely.git"
  },
  "author": {
    "name": "Pelle Wessman",
    "email": "pelle@kodfabrik.se",
    "url": "http://kodfabrik.se/"
  },
  "license": "MIT",
  "dependencies": {
    "bunyan-adaptor": "^3.0.0",
    "lodash.isequal": "^4.4.0",
    "lodash.pull": "^4.1.0",
    "lru-cache": "^4.0.0",
    "request": "^2.87.0",
    "robots": "^0.10.0",
    "verror": "^1.6.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "main": "./lib/main.js",
  "directories": {
    "example": "./example",
    "lib": "./lib"
  },
  "scripts": {
    "dependency-check": "JS_FILES=\"example/*.js lib/*.js\" && dependency-check . $JS_FILES && dependency-check . $JS_FILES --unused --no-dev",
    "test": "installed-check -e -i eslint . && eslint . && npm run --silent dependency-check && tsc"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "@voxpelli/eslint-config": "4.0.0",
    "dependency-check": "3.4.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "^21.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "1.3.1",
    "installed-check": "2.2.0",
    "typescript": "3.7.2"
  }
}
